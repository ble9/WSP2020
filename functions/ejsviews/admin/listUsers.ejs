<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <%- include('../partials/header.ejs') %>

        <title>List  admin</title>
</head>
<body>
    <h1>List of Users</h1>
    <a href= "/admin/listUsers.ejs" class ="btn btn-outline-primary"> System Admin Home</a> 

    <% if (!error) { %>
        <br>
        <% users.forEach (u => { %> 
            Email: <%= u.email %> <br>
            DisplayName <%= u.displayName %> <br>
            PhoneNumber <%= u.PhoneNumber %><br>
            <img src = "<%= u.photoURL%> "> <br> <br>

        <% }) %>
<% } else { %>
<%= error %> 
<% } %> 

    <%- include('../partials/scripts.ejs') %>
</body>

</html>